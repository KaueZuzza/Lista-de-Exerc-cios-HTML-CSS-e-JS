        var fahrenheit = (celsius * 9/5) + 32;

        alert("- O resultado da conversao de C para F foi: "+fahrenheit);

        console.log("- O valor em Celsius foi: "+celsius);
        console.log("- O valor em Fahrenheit foi: "+fahrenheit);
    </script>
