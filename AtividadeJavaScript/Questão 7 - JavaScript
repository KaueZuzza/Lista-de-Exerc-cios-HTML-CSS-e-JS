<body>
    <script>
        var Num = prompt("Digite um número inteiro:");

        for (var n = 2; n <= Num; n++) {
        var primo = true;

        for (var i = 2; i < n; i++) {
            if (n % i == 0) {
            primo = false;
            }
        }

        if (primo) {
            console.log("- "+n);
        }
        }

    </script>
</body>
